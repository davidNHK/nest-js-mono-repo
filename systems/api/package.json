    "url": "git@github.com/davidNHK/project-bootstrap"
  },
  "scripts": {
    "build": "npx babel --config-file ./.babelrc.esm.mjs --out-dir ./dist/ --extensions .ts ./src/",
    "build:test": "npx babel --config-file ./.babelrc.cjs.cjs --out-file-extension .cjs --out-dir ./cjs-dist/ --extensions .ts ./src/",
    "eslint": "eslint --ext=json,ts,yml",
    "lint": "npm run eslint -- .",
    "migration:create": "npx typeorm migration:create --config ./ormconfig.cli.cjs",
    "migration:down": "npx typeorm migration:revert --config ./ormconfig.cli.cjs",
    "migration:up": "npx typeorm migration:run --config ./ormconfig.cli.cjs",
    "prebuild": "rimraf dist",
    "seed:create": "npx typeorm migration:create --config ./ormconfig.seed.cli.cjs",
    "seed:up": "npx typeorm migration:run --config ./ormconfig.seed.cli.cjs",
    "start:dev": "npm run build && node ./dist/main.js",
    "start:prod": "node ./dist/main.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --maxWorkers=50%",
    "test:ci": "NODE_OPTIONS=--experimental-vm-modules NODE_ENV=test jest --ci --coverage --runInBand --bail",
    "typeorm:dev": "NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js",
    "watch:build": "NODE_ENV=development nodemon ./dist/main.js"
  },
  "type": "module",
  "version": "0.2.0"
}
