  "license": "MIT",
  "name": "api",
  "private": true,
  "repository": {
    "directory": "apps/api",
    "url": "git@github.com:davidNHK/nest-js-monorepo.git"
  },
  "scripts": {
    "build": "nest build",
    "eslint": "eslint --ext=json,ts",
    "lint": "npm run eslint -- .",
    "migration:create": "npx typeorm migration:create --config ./ormconfig.cli.js",
    "migration:down": "npx typeorm migration:revert --config ./ormconfig.cli.js",
    "migration:up": "npx typeorm migration:run --config ./ormconfig.cli.js",
    "prebuild": "rimraf dist",
    "precommit": "lint-staged",
    "seed:create": "npx typeorm migration:create --config ./ormconfig.seed.cli.js",
    "seed:up": "npm run typeorm:dev -- migration:run --config ./ormconfig.seed.cli.js",
    "start:dev": "NODE_ENV=development npm run migration:up && npm run seed:up && NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main",
    "test": "NODE_ENV=test jest --maxWorkers=50%",
    "test:ci": "NODE_ENV=test jest --ci --coverage --runInBand",
    "typeorm:dev": "NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js"
  },
  "version": "0.2.0"
}
